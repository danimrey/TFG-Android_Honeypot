<html>
	<head>
		<title>HTTP uploads</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<!--[if lte IE 8]><script src="assets/js/ie/html5shiv.js"></script><![endif]-->
		<link rel="stylesheet" href="assets/css/main.css" />
		<link href="icon.png" type="image/x-icon" rel="shortcut icon" />
		<!--[if lte IE 9]><link rel="stylesheet" href="assets/css/ie9.css" /><![endif]-->
		<!--[if lte IE 8]><link rel="stylesheet" href="assets/css/ie8.css" /><![endif]-->
	</head>
	<body>
		<div id="page-wrapper">

			<!-- Header -->
				<header id="header">
					<h1 id="logo"><a href="/">Android Honeypot Monitor</a></h1>
					<nav id="nav">
						<ul>
							<li><a href="/">Logs recientes</a></li>
							<li>
								<a href="#">Logs por servicio</a>
								<ul>
									<li><a href="/telnet">Telnet</a></li>
									<li><a href="/ftp">FTP</a></li>
									<li>
										<a href="/http">HTTP</a>
										<ul>
											<li><a href="/uploads">Archivos subidos</a></li>
										</ul>
									</li>
								</ul>
							</li>
							<li><a href="/expulsados">Expulsados</a></li>
							<li><a href="/buscar" class="button special">Buscar</a></li>
						</ul>
					</nav>
				</header>

			<!-- Main -->
				<div id="main" class="wrapper style1">
					<div class="container">
						<header class="major">
							<h2>Archivos subidos al servidor HTTP</h2>
						</header>

								<!-- Table -->
							<section id="HTTP">
								<% if (obj.logs.length != 0){ %>
									<div class="table-wrapper">
									<table class="alt">
										<thead>
											<tr>
												<th>Fecha</th>
												<th>IP atacante</th>
												<th>Puerto atacante</th>
												<th>IP servidor</th>
												<th>Puerto servidor</th>
												<th>Nombre archivo</th>
												<th>Tipo de archivo</th>
												<th>Ruta</th>
												<th>Tama√±o</th>
											</tr>
										</thead>
										<tbody>
											<% for(var t in obj.logs){ %>
												<tr>
													<% var d = obj.logs[t].createdAt%>
													<% var formatted = d.toLocaleString() %>
													<td><%= formatted%></td>
													<td><%= obj.logs[t].AtacanteIP %></td>
													<td><%= obj.logs[t].AtacantePuerto %></td>
													<td><%= obj.logs[t].ServerIP %></td>
													<td><%= obj.logs[t].ServerPuerto %></td>
													<td><%= obj.logs[t].Archivo %></td>
													<td><%= obj.logs[t].Tipo %></td>
													<td><%= obj.logs[t].Path %></td>
													<td>
													<% 
														function formatBytes(bytes,decimals) {
														   if(bytes == 0) return '0 Byte';
														   var k = 1000; // or 1024 for binary
														   var dm = decimals + 1 || 3;
														   var sizes = ['Bytes', 'KB', 'MB', 'GB', 'TB', 'PB', 'EB', 'ZB', 'YB'];
														   var i = Math.floor(Math.log(bytes) / Math.log(k));
														   return parseFloat((bytes / Math.pow(k, i)).toFixed(dm)) + ' ' + sizes[i];
														}
														var tamano = formatBytes(obj.logs[t].Size, 2);
													%>
													<%= tamano%>
													</td>
											</tr>

											<% } %>
										</tbody>
									</table>
									</div>
								<% }else{ %>
									<p>No hay logs de servidor HTTP</p>
								<% } %>
							</section>
					</div>
				</div>

			<!-- Footer -->
				<footer id="footer">
					<ul class="icons">
						<li><a href="#" class="icon alt fa-twitter"><span class="label">Twitter</span></a></li>
						<li><a href="https://github.com/danimrey/TFG-Android_Honeypot" class="icon alt fa-github"><span class="label">GitHub</span></a></li>
						<li><a href="mailto:daniel.moreno.rey@alumnos.upm.es" class="icon alt fa-envelope"><span class="label">Email</span></a></li>
					</ul>
					<ul class="copyright">
						<li>TFG - Daniel Moreno Rey</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
					</ul>
				</footer>

		</div>

		<!-- Scripts -->
			<script src="assets/js/jquery.min.js"></script>
			<script src="assets/js/jquery.scrolly.min.js"></script>
			<script src="assets/js/jquery.dropotron.min.js"></script>
			<script src="assets/js/jquery.scrollex.min.js"></script>
			<script src="assets/js/skel.min.js"></script>
			<script src="assets/js/util.js"></script>
			<!--[if lte IE 8]><script src="assets/js/ie/respond.min.js"></script><![endif]-->
			<script src="assets/js/main.js"></script>

	</body>
</html>